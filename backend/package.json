{
  "name": "rocketrealtor-backend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
"scripts": {
  "postinstall": "npx prisma generate --schema=prisma/schema.prisma",
  "dev":    "nodemon server/server.js",
  "start":  "node server/server.js",
  "seed":   "node prisma/seed.js",
  "migrate:dev":    "npx prisma migrate dev --schema=prisma/schema.prisma",
  "migrate:deploy": "npx prisma migrate deploy --schema=prisma/schema.prisma"
},

  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "socket.io": "^4.7.4",
    "@prisma/client": "^5.8.1",
    "node-cron": "^3.0.3",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "csv-parse": "^5.6.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "prisma": "^5.8.1",
    "nodemon": "^2.0.0"
  }
}